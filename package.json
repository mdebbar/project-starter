  "lint-staged": {
    "*.{js,jsx}": ["eslint --no-ignore --fix", "git add --force"],
    "*.{json,md,graphql,css,less,scss,sass}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "fix": "yarn run lint-js --fix",
    "check": "flow check",
    "test": "jest",
    "test-watch": "yarn run test --watch --notify",
    "test-cover": "yarn run test --coverage",
    "coverage": "yarn run test-cover && opn coverage/lcov-report/index.html",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build",
    "build-stats": "yarn run build --release --analyse",
    "deploy": "babel-node tools/run deploy",
    "render": "babel-node tools/run render",
    "serve": "babel-node tools/run runServer",
    "start": "babel-node tools/run start --silent",
    "debug": "babel-node --inspect tools/run start --silent",
    "console": "babel-node",
    "sqlz": "babel-node ./node_modules/.bin/sequelize",
    "db:test-connection": "babel-node tools/run db/test-connection",
    "db:create": "babel-node tools/run db/create",
    "db:migrate": "yarn sqlz db:migrate",
    "db:migrate:undo": "yarn sqlz db:migrate:undo",
    "db:migration:create": "yarn sqlz migration:create",
    "db:migration:status": "yarn sqlz db:migrate:status"
  }
}
